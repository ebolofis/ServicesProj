<div>
    <div class="pure-container" data-effect="pure-effect-slide">
        <input type="checkbox" style="visibility:hidden" id="pure-toggle-left" class="pure-toggle" data-toggle="left" />

        <label id="hamburgericon" style="" class="pure-toggle-label" for="pure-toggle-left" data-toggle-label="left"><span class="pure-toggle-icon"></span></label>

        <nav class="pure-drawer" data-position="left">
            <p style="padding: 50px 20px; margin: 0; "></p>
            <p class="paragraphstyling" onclick="redirectToPlugins()">Plugins<p />
            <p class="paragraphstyling" onclick="redirectToConfiguration(1)">Main Configuration<p />
            <p class="paragraphstyling" onclick="redirectToServices()">Services<p />
            <p class="paragraphstyling" onclick="redirectToScheduler()">Scheduler<p />
            <p class="paragraphstyling" onclick="redirectToInitializeDB()">Initialize Database<p />
            <p class="paragraphstyling" onclick="redirectToSMTPConfiguration()">SMTP Configuration<p />
            <details style="color:white;cursor:pointer">
                 <summary>Internal Services</summary>
                <ul>
                    <li>   <p class="paragraphstyling" onclick="redirectToExportDataServices()">Export Data Services<p /></li>
                    <li>  <p class="paragraphstyling" onclick="redirectToReadCsvServices()">Read Csv Services<p /></li>
                    <li> <p class="paragraphstyling" onclick="redirectToSaveToTableServices()">Save To Table Services<p /></li>
                    <li>  <p class="paragraphstyling" onclick="redirectToSqlScriptsServices()">Sql Scripts Services<p /></li>
                </ul>
            </details>
         
          
           
          
            <p class="paragraphstyling" onclick="redirectToLogin()">Logout<p />
        </nav>

        <label class="pure-overlay" for="pure-toggle-left" data-overlay="left"></label>
        @RenderBody()
    </div>
</div>

